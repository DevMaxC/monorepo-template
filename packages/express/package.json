{
  "name": "express-app",
  "version": "1.0.0",
  "description": "Express server for SST application using TypeScript",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "npx dotenv-cli -e .env -- nodemon --watch 'src/**/*.ts' --exec 'tsx' src/server.ts",
    "watch": "tsc --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "sst": "^3.11.11",
    "twilio": "^5.5.1",
    "ws": "^8.18.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.17",
    "@types/node": "^20.8.0",
    "@types/ws": "^8.18.0",
    "dotenv-cli": "^7.4.1",
    "nodemon": "^3.1.0",
    "tsx": "^4.7.1",
    "typescript": "^5.2.2"
  }
}
